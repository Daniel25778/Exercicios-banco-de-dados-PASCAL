<?xml version="1.0" encoding="UTF-8"?>
<Application xmlns="http://schemas.microsoft.com/windows/2009/Ribbon">
  <Application.Commands>
    <Command Name="cmdTabSistema" LabelTitle="Sistema"/>
    <Command Name="cmdTabEditar" LabelTitle="Editar"/>
    <Command Name="cmdTabConceitual" LabelTitle="Esquema Conceitual"/>
    <Command Name="cmdTabLogico" LabelTitle="Esquema Lógico"/>
    <Command Name="cmdTabConfiguracao" LabelTitle="Configurações"/>
    <Command Name="cmdGroupNovo" LabelTitle="Novo"/>
    <Command Name="cmdGroupArquivo" LabelTitle="Arquivo"/>
    <Command Name="cmdGroupExportacao" LabelTitle="Exportação"/>
    <Command Name="cmdGroupImpressao" LabelTitle="Impressão"/>
    <Command Name="cmdGroupOpcoes" LabelTitle="Opções"/>
    <Command Name="cmdGroupFerramentasConceitual" LabelTitle="Ferramentas"/>
    <Command Name="cmdGroupFerramentasLogico" LabelTitle="Ferramentas"/>
    <Command Name="cmdGroupComandosConceitual" LabelTitle="Comandos"/>
    <Command Name="cmdGroupComandosLogico" LabelTitle="Comandos"/>
    <Command Name="cmdNovoConceitual" LabelTitle="Modelo Conceitual"/>
    <Command Name="cmdNovoLogico" LabelTitle="Modelo Lógico"/>
    <Command Name="cmdAbrir" LabelTitle="Abrir"/>
    <Command Name="cmdSalvar" LabelTitle="Salvar"/>
    <Command Name="cmdSalvarComo" LabelTitle="Salvar Como ..."/>
    <Command Name="cmdFechar" LabelTitle="Fechar"/>
    <Command Name="cmdExportarJPEG" LabelTitle="Exportar em JPEG"/>
    <Command Name="cmdExportarBMP" LabelTitle="Exportar em BMP"/>
    <Command Name="cmdImprimir" LabelTitle="Imprimir"/>
    <Command Name="cmdGerarDicionario" LabelTitle="Gerar Dicionário do Esquema"/>
    <Command Name="cmdAjuda" LabelTitle="Ajuda"/>
    <Command Name="cmdConfiguracoes" LabelTitle="Configurações"/>
    <Command Name="cmdCancelarConceitual" LabelTitle="Cancelar"/>
    <Command Name="cmdCancelarLogico" LabelTitle="Cancelar"/>
    <Command Name="cmdAutoSalvar" LabelTitle="Auto Salvar"/>
  </Application.Commands>
  <Application.Views>
    <Ribbon>
      <Ribbon.ApplicationMenu>
        <ApplicationMenu/>
      </Ribbon.ApplicationMenu>
      <Ribbon.HelpButton>
        <HelpButton CommandName="cmdAjuda"/>
      </Ribbon.HelpButton>
      <Ribbon.QuickAccessToolbar>
        <QuickAccessToolbar/>
      </Ribbon.QuickAccessToolbar>
      <Ribbon.Tabs>
        <Tab CommandName="cmdTabSistema">
          <Group CommandName="cmdGroupNovo" SizeDefinition="TwoButtons">
            <Button CommandName="cmdNovoConceitual"/>
            <Button CommandName="cmdNovoLogico"/>
          </Group>
          <Group CommandName="cmdGroupArquivo" SizeDefinition="FourButtons">
            <Button CommandName="cmdAbrir"/>
            <Button CommandName="cmdSalvar"/>
            <Button CommandName="cmdSalvarComo"/>
            <Button CommandName="cmdFechar"/>
          </Group>
          <Group CommandName="cmdGroupExportacao" SizeDefinition="TwoButtons">
            <Button CommandName="cmdExportarJPEG"/>
            <Button CommandName="cmdExportarBMP"/>
          </Group>
          <Group CommandName="cmdGroupImpressao" SizeDefinition="TwoButtons">
            <Button CommandName="cmdImprimir"/>
            <Button CommandName="cmdGerarDicionario"/>
          </Group>
        </Tab>
        <Tab CommandName="cmdTabConceitual">
          <Group CommandName="cmdGroupFerramentasConceitual" SizeDefinition="OneButton">
            <Button CommandName="cmdCancelarConceitual"/>
          </Group>
        </Tab>
        <Tab CommandName="cmdTabLogico">
          <Group CommandName="cmdGroupFerramentasConceitual" SizeDefinition="OneButton">
            <Button CommandName="cmdCancelarLogico"/>
          </Group>
        </Tab>
        <Tab CommandName="cmdTabConfiguracao">
          <Group CommandName="cmdGroupOpcoes" SizeDefinition="TwoButtons">
            <Button CommandName="cmdConfiguracoes"/>
            <Button CommandName="cmdAutoSalvar"/>
          </Group>
        </Tab>
      </Ribbon.Tabs>
    </Ribbon>
    <ContextPopup/>
  </Application.Views>
</Application>